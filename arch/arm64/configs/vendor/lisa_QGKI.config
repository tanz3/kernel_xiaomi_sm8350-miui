# Fingerprint
CONFIG_FINGERPRINT_FPC_TEE=y

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW8624_HAPTIC=y

# Pinctrl
CONFIG_PINCTRL_SM7325=y

# Power Supply
CONFIG_BQ2597X=y

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_BRL=y
CONFIG_TOUCHSCREEN_GOODIX_TS_GESTURE=y
CONFIG_TOUCHSCREEN_GOODIX_TS_TOOLS=y
CONFIG_TOUCHSCREEN_QGKI_GOODIX=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y

# WiFi
CONFIG_CNSS_QCA6750=y
