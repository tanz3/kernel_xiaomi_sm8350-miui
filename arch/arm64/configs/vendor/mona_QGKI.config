# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=y

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW862XX_HAPTIC=y

# Power Supply
CONFIG_BQ2597X=y
CONFIG_BQ_FUEL_GAUGE=y

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Touchscreen
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_S3908P=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_SPI=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_CORE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_REFLASH=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_DEVICE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_TESTING=y
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM_DIAGNOSTIC=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y

# WiFi
CONFIG_CNSS_QCA6750=y
